package unq.cazaDeVinchucas.modelo;

import static org.mockito.Mockito.mock;
import static org.mockito.Mockito.verify;
import static org.mockito.Mockito.when;

import java.io.File;

import org.junit.jupiter.api.BeforeEach;

import main.java.unq.cazaDeVinchucas.modelo.ENormal;
import main.java.unq.cazaDeVinchucas.modelo.Muestra;
import main.java.unq.cazaDeVinchucas.modelo.Opinion;
import main.java.unq.cazaDeVinchucas.modelo.Ubicacion;
import main.java.unq.cazaDeVinchucas.modelo.Usuario;

public class MuestraTest {
	
	Opinion OpinionMock;
	
	Usuario UsuarioMock;
	File FileMock;
	Ubicacion UbicacionMock;
	ENormal EnormalMock;
	
	Muestra MuestraP;
	
	@BeforeEach
	public void setUp() {
		OpinionMock = mock(Opinion.class);
		
		UsuarioMock = mock(Usuario.class);
		FileMock = mock(File.class);
		UbicacionMock  = mock(Ubicacion.class);
		EnormalMock = mock(ENormal.class);
		
		MuestraP = new Muestra(UsuarioMock, FileMock, UbicacionMock, EnormalMock);
	}
	
	public void Opinion() {
		MuestraP.agregarOpinion(OpinionMock);
		
		
		
		verify(EnormalMock).corroborarEstado();
	}
	
}
